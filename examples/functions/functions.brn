export fn greet(name: string) {
    print(name);
}

export fn add(a: int, b: int) -> int {
    return a + b;
}

export fn is_between(val: int, lo: int, hi: int) -> bool {
    return val >= lo && val <= hi;
}

export fn fib(n: int) -> int {
    if n < 2 {
        return n;
    }
    return fib(n - 1) + fib(n - 2);
}
